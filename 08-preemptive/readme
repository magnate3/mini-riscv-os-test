    # Set mstatus.MPP to 3, so we still run in Machine mode after MRET.
    # Set mstatus.MPIE to 1, so MRET will enable the interrupt.
    li      t0, 3 << 11 | 1 << 7
    csrr    a1, mstatus
    or      t0, t0, a1
    csrw    mstatus, t0
